{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Test",
      "request": "launch",
      "type": "node",
      "program": "${workspaceRoot}/node_modules/.bin/_mocha",
      "args": [
        "dist/temp/exceptionless-spec.js"
      ],
      "cwd": "${workspaceRoot}/",
      "runtimeArgs": [
        "--nolazy"
      ],
      "sourceMaps": false,
      "outDir": "${workspaceRoot}/dist"
    },
    {
      "name": "Run",
      "request": "launch",
      "type": "node",
      "program": "${workspaceRoot}/test.js",
      "runtimeArgs": [
        "--nolazy"
      ],
      "cwd": "${workspaceRoot}/"
    },
    {
      "name": "Express",
      "request": "launch",
      "type": "node",
      "program": "${workspaceRoot}/example/express/app.js",
      "runtimeArgs": [
        "--nolazy"
      ],
      "cwd": "${workspaceRoot}/",
      "sourceMaps": true,
      "outDir": "${workspaceRoot}/dist"
    },
    {
      "name": "Attach",
      "request": "attach",
      "type": "node",
      "port": 5858,
      "sourceMaps": true,
      "outDir": "${workspaceRoot}/dist"
    }
  ]
}
